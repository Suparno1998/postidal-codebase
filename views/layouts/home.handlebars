<!DOCTYPE html>
<html lang="en">

  <head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="format-detection" content="telephone=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="author" content="Postidal">
    <meta name="keywords" content="{{metaData.keywords}}">
    <meta name="description" content="{{metaData.description}}">
    <title>{{metaData.title}}</title>
    <link href="https://fonts.googleapis.com/css?family=Work+Sans:300,400,500,600,700&amp;amp;subset=latin-ext" rel="stylesheet">
    <link rel="stylesheet" href="{{config.root}}fonts/Linearicons/Linearicons/Font/demo-files/demo.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="{{config.root}}plugins/owl-carousel/assets/owl.carousel.css">
    <link rel="stylesheet" href="{{config.root}}plugins/slick/slick/slick.css">
    <link rel="stylesheet" href="{{config.root}}plugins/lightGallery-master/dist/css/lightgallery.min.css">
    <link rel="stylesheet" href="{{config.root}}plugins/jquery-bar-rating/dist/themes/fontawesome-stars.css">
    <link rel="stylesheet" href="{{config.root}}plugins/jquery-ui/jquery-ui.min.css">
    <link rel="stylesheet" href="{{config.root}}plugins/select2/dist/css/select2.min.css">
    <link rel="stylesheet" href="{{config.root}}css/style.css">
    <link rel="stylesheet" href="{{config.root}}css/electronic.css">

  </head>

  <body>
    
    {{>home/home-nav}}

    <div class="mainContainer">


        {{#if errors}}
            {{#each errors}}
                <div class="alert-danger">
                    {{message}}
                </div>
            {{/each}}
        {{/if}}

        {{#if success_message}}
          <div class="alert alert-success alert-dismissible fade show mt-2">
            <button class="close" data-dismiss="alert" type="button">
                <span>&times;</span>
            </button>
            <strong>{{success_message}}</strong>
          </div>
        {{/if}}

        {{#if error_message}}
            <div class="alert alert-danger">
                <strong>{{error_message}}</strong>
            </div>
        {{/if}}
      

    	{{{body}}}



    </div>

    {{>home/footer}}

    <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
    <script src="{{config.root}}plugins/owl-carousel/owl.carousel.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <script src="{{config.root}}plugins/imagesloaded.pkgd.min.js"></script>
    <script src="{{config.root}}plugins/masonry.pkgd.min.js"></script>
    <script src="{{config.root}}plugins/isotope.pkgd.min.js"></script>
    <script src="{{config.root}}plugins/jquery.matchHeight-min.js"></script>
    <script src="{{config.root}}plugins/slick/slick/slick.min.js"></script>
    <script src="{{config.root}}plugins/jquery-bar-rating/dist/jquery.barrating.min.js"></script>
    <script src="{{config.root}}plugins/slick-animation.min.js"></script>
    <script src="{{config.root}}plugins/lightGallery-master/dist/js/lightgallery-all.min.js"></script>
    <script src="{{config.root}}plugins/jquery-ui/jquery-ui.min.js"></script>
    <script src="{{config.root}}plugins/sticky-sidebar/dist/sticky-sidebar.min.js"></script>
    <script src="{{config.root}}plugins/jquery.slimscroll.min.js"></script>
    <script src="{{config.root}}plugins/select2/dist/js/select2.full.min.js"></script>
    <script src="{{config.root}}js/main.js"></script>
    <script src="https://use.fontawesome.com/62e26e8a6e.js"></script>
  </body>

</html>
